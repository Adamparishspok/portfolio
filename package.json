{
  "name": "atomparish",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "watch:html": "eleventy --serve --incremental",
    "build:html": "eleventy",
    "dev:css": "tailwindcss -i src/assets/css/main.css -o _site/assets/css/main.css",
    "watch:css": "tailwindcss -i src/assets/css/main.css -o _site/assets/css/main.css --watch",
    "build:css": "tailwindcss -i src/assets/css/main.css -o _site/assets/css/main.css --minify",
    "watch:all": "pnpm run watch:html & pnpm run watch:css",
    "start": "pnpm run clean && pnpm run dev:css && pnpm run watch:all",
    "build": "pnpm run clean && pnpm run build:html && pnpm run build:css",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test:a11y": "pa11y-ci"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/cli": "^4.1.17",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "html-minifier-terser": "^7.2.0",
    "pa11y-ci": "^4.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.17"
  },
  "dependencies": {
    "geist": "^1.5.1",
    "gsap": "^3.13.0",
    "lucide": "^0.554.0"
  }
}
